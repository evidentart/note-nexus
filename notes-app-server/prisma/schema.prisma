generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// effectively going to be a table and our database called Note , give properties we want to store

model Note {
  id Int @id @default(autoincrement())
  title String
  content String
}
